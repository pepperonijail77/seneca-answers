{
	"manifest_version": 2,
	"name": "Seneca Answers",
	"version": "2.0.1",
	"developer": {
		"name": "pepperonijail77",
		"url": "https://github.com/pepperonijail77"
	},
	"description": "Web extension to get the answers to SenecaLearning lessons",
	"permissions": ["webRequest", "https://*.senecalearning.com/*"],
	"background": {"scripts": ["background.js"]},
	"content_scripts": [
		{
			"matches": ["https://app.senecalearning.com/*"],
			"css": ["overlay.css"],
			"js": ["content.js"]
		}
	],
	"icons": {
		"32": "icons/icon-32.png",
		"64": "icons/icon-64.png",
		"192": "icons/icon-192.png"
	}
}
